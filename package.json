{
  "name": "@yjhtry/create-all",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@8.15.6",
  "description": "create all kinds of project templates use one command",
  "license": "MIT",
  "homepage": "https://github.com/yjhtry/create-all#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjhtry/create-all.git"
  },
  "bugs": {
    "url": "https://github.com/yjhtry/create-all/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "create-all": "bin/create-all.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=create-all esno ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "typecheck": "tsc",
    "prepublishOnly": "nr build",
    "release": "pnpm build && bumpp && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@npmcli/config": "^8.2.2",
    "cli-progress": "^3.12.0",
    "deepmerge": "^4.3.1",
    "execa": "^8.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@antfu/utils": "^0.7.7",
    "@types/cli-progress": "^3.11.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.12.7",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "@yjhtry/types-func": "^0.0.6",
    "bumpp": "^9.4.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
